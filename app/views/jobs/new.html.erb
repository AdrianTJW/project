<div class="row">
<n3 style="font-size: 40px;">Create a new job! <a href="/jobs"><button class="right small success button">Back</button></a></n3>

    <script>
      function readURL(input) {
        if (input.files && input.files[0]) {
          var reader = new FileReader();

          reader.onload = function (e) {
            $('#img_prev')
              .attr('src', e.target.result)
              .height(250);
          };

          reader.readAsDataURL(input.files[0]);
        }
      }
    </script>
<%= render 'form' %>
</div>

